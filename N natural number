// Write a recursive function to print first N nartural numbers.
#include<iostream>
using namespace std;
void N_natural(int a)
{
    if(a!=0)
    {
        N_natural(a-1);
    }
    else
    {
        return;
    }
    cout << a << "\t";
}
int main()
{
    int n;
    cout << "Enter a number :  ";
    cin >> n;
    cout << "-----------Series--------------\n";
    N_natural(n);
}
